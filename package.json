    "json2csv": "5.0.6",
    "jwt-decode": "4.0.0",
    "lodash": "4.17.21",
    "mjml": "4.15.3",
    "moment": "2.30.1",
    "mongodb": "3.7.4",
    "mongodb-core": "3.2.7",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.9.9",
    "nodemailer-html-to-text": "3.2.0",
    "pg": "8.11.3",
    "puppeteer": "22.11.2",
    "serve-favicon": "2.5.0",
    "sib-api-v3-sdk": "8.3.0",
    "slugify": "1.6.6",
    "uuid": "9.0.1",
    "winston": "3.11.0",
    "winston-logstash": "1.2.1"
  },
  "engines": {
    "node": ">= 19.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "eslint": "8.56.0",
    "git-precommit-checks": "3.1.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "mocha": "10.3.0",
    "nodemon": "3.0.3"
  }
