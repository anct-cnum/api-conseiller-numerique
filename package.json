    "slugify": "1.4.6",
    "uuid": "8.3.2",
    "winston": "3.3.3",
    "winston-logstash": "0.4.0"
  },
  "engines": {
    "node": ">= 14.15.3"
  },
  "devDependencies": {
    "axios": "0.21.1",
    "eslint": "7.24.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7"
  }
}
