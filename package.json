{
  "name": "api-conseiller-numerique",
  "version": "0.0.2",
  "description": "API Conseiller Numérique",
  "main": "index.js",
  "author": "Loïc Guillois <loic@fitlab.fr>",
  "contributors": [
    "Loïc Guillois <loic@fitlab.fr>",
    "Jean-Yves Stervinou <jy@stervinou.com>"
  ],
  "license": "MIT",
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --fix",
    "dev": "nodemon src/",
    "start": "node src/",
    "test:integration": "mocha test/integration --recursive --exit",
    "test:config": "mocha test/config --recursive --exit",
    "smtp:start": "docker rm cnum-smtp; docker run -d --name cnum-smtp -p 1025:1025 -p 8025:8025 mailhog/mailhog",
    "smtp:stop": "docker stop cnum-smtp"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/authentication": "4.5.11",
    "@feathersjs/authentication-local": "4.5.11",
    "@feathersjs/authentication-oauth": "4.5.11",
    "@feathersjs/configuration": "4.5.11",
    "@feathersjs/errors": "4.5.11",
    "@feathersjs/express": "4.5.11",
    "@feathersjs/feathers": "4.5.11",
    "@feathersjs/transport-commons": "4.5.11",
    "@hapi/boom": "9.1.1",
    "@sentry/node": "6.3.3",
    "@sentry/tracing": "6.3.3",
    "@turf/circle": "6.3.0",
    "axios": "0.21.1",
    "commander": "6.2.1",
    "compression": "1.7.4",
    "config": "3.3.3",
    "cors": "2.8.5",
    "csv-parse": "4.15.3",
    "csvtojson": "2.0.10",
    "dotenv": "8.2.0",
    "ejs": "3.1.5",
    "eslint": "7.24.0",
    "excel4node": "1.7.2",
    "exceljs": "4.2.0",
    "feathers-mongodb": "6.3.0",
    "feathers-mongodb-fuzzy-search": "2.0.1",
    "feathers-permissions": "2.1.4",
    "helmet": "4.4.1",
    "joi": "17.3.0",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "mjml": "4.10.1",
    "moment": "2.29.1",
    "mongodb": "3.6.3",
    "mongodb-core": "3.2.7",
    "nodemailer": "6.4.17",
    "nodemailer-html-to-text": "3.1.0",
    "pg": "8.5.1",
    "puppeteer": "10.1.0",
